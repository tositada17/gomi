{
  "name": "gomi",
  "displayName": "gomi",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands":[
      {
        "command":"gomi.helloWorld",
        "title":"hello World"
      },
      {
        "command":"gomi.infoMessage",
        "title":"info Message"
      },
    ],
    "configuration": {
      "properties": {
        "gomi.traceLevel": {
          "type": "string",
          "default": "off",
          "description": "The trace level set to see gomi log from output window",
          "enum": [
            "off",
            "error",
            "warning",
            "info",
            "verbose"
          ]
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@vscode/vsce": "^3.2.2",
    "generator-code": "^1.11.10",
    "mocha": "^11.7.1",
    "typescript-eslint": "^8.38.0"
  }
}
